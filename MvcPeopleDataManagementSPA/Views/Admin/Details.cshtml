@model IdentityAppUser
@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<dl>
    <dt>Username</dt>
    <dd>@Model.UserName</dd>

    <dt>Email</dt>
    <dd>@Model.Email</dd>

    <dt>PhoneNumber</dt>
    <dd>@Model.PhoneNumber</dd>

    <dt>LockoutEnabled</dt>
    <dd>@Model.LockoutEnabled</dd>
</dl>


@Html.ActionLink("|Roles Manager|", "RolesManagement", "Admin", new { id = Model.Id })
<a asp-action="UserList" style="margin-left: 20px">|Back|</a>